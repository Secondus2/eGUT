<?xml version="1.0" encoding="UTF-8"?>
<document>
	<simulation name="microcosm" outputfolder="../results" randomSeed="22" log="DEBUG">
		<timer stepSize="1.0" endOfSimulation="100.0" />
		<speciesLib>
			<species name="Ecoli">
				<aspect name="density" type="PRIMARY" class="Double" value="1.105" />
				<aspect name="emax" type="PRIMARY" class="Double" value="2.0" />
				<aspect name="hillcoef" type="PRIMARY" class="Double" value="2.0" />
				<aspect name="fitness_cost" type="PRIMARY" class="Double" value="0.0" />
				<aspect name="reactions" type="PRIMARY" class="InstantiableList"> 
					<list class="Reaction" nodeLabel="reaction" entryClass="Reaction" keyClass="String" keyLabel="name">
						<reaction name="growth">
							<expression value="(1.0-(emax*(antibiotic^hillcoef)/((mic^hillcoef)+(antibiotic^hillcoef))))*(1.0-fitness_cost)*mumax*mass*solute1/(Ks+solute1)">
								<constant name="Ks" value="3.2707e-005" />
								<constant name="mumax" value="0.5" />
							</expression>
							<stoichiometric component="mass" coefficient="3.0" />
							<stoichiometric component="solute1" coefficient="-1.0" />
						</reaction>
						<reaction name="death" >
							<expression value="delta*mass" >
								<constant name="delta" value="0.025" />
							</expression>
							<stoichiometric component="mass" coefficient="-1.0" />
							<stoichiometric component="solute1" coefficient="2.0" />
						</reaction>
					</list>
				</aspect>
				<speciesModule name="coccoid" />
			</species>
			<species name="coccoid">
				<aspect name="surfaces" type="CALCULATED" class="AgentSurfaces" />
				<aspect name="#isLocated" type="PRIMARY" class="Boolean" value="true" />
				<aspect name="morphology" type="PRIMARY" class="String" value="coccoid" />
				<aspect name="volume" type="CALCULATED" class="StateExpression" input="mass/density" />
				<aspect name="radius" type="CALCULATED" class="CoccoidRadius" />
				<aspect name="divide" type="EVENT" class="CoccoidDivision" />
				<aspect name="updateBody" type="EVENT" class="UpdateBody" />
				<aspect name="searchDist" type="PRIMARY" class="Double" value="0.0"  />
			</species>
		</speciesLib>
		<compartment name="microcosm">
			<shape class="Rectangle">
				<dimension name="X" isCyclic="true" targetResolution="1.0" max="300.0"/>
				<dimension name="Y" isCyclic="true" targetResolution="1.0" max="300.0"/>
			</shape>
			<solutes>
				<solute name="solute1" concentration="0.03271" />
				<solute name="antibiotic" concentration="5.6" />
			</solutes>
			<agents>
				<agent>
					<aspect name="species" type="PRIMARY" class="String" value="Ecoli" />
					<aspect name="mass" type="PRIMARY" class="Double" value="0.5" />
					<aspect name="mic" type="PRIMARY" class="Double" value="8.0" />
					<aspect name="pigment" type="PRIMARY" class="String" value="GREEN" />
					<spawn number="800" domain="300.0,300.0" />
				</agent>
				<agent>
					<aspect name="species" type="PRIMARY" class="String" value="Ecoli" />
					<aspect name="mass" type="PRIMARY" class="Double" value="0.1" />
					<aspect name="pigment" type="PRIMARY" class="String" value="RED" />
					<aspect name="betaLacRes" type="PRIMARY" class="HashMap">
						<item type="PRIMARY" class="Double" value="0.75" key="transfer_probability" keyType="String" />
						<item type="PRIMARY" class="Double" value="0.001" key="transfer_frequency" keyType="String" />
						<item type="PRIMARY" class="Double" value="0.10" key="loss_probability" keyType="String" />
						<item type="PRIMARY" class="Double" value="1.0" key="copy" keyType="String" />
						<item type="PRIMARY" class="Double" value="5.6" key="pili_length" keyType="String" />
						<item type="PRIMARY" class="String" value="BLUE" key="loss_pigment" keyType="String" />
					</aspect>
					<aspect name="fitness_cost" type="PRIMARY" class="Double" value="0.1" />
					<aspect name="mic" type="PRIMARY" class="Double" value="2000.0" />
					<aspect name="cool_down" type="PRIMARY" class="Double" value="0.2" />
					<spawn number="10" domain="300.0,300.0" />
				</agent>
			</agents>
			<processManagers>
				<process name="svgWriter" class="GraphicalOutput"  priority="-4" firstStep="0.0"  timerStepSize="0.2">
					<aspect name="solute" type="PRIMARY" class="String" value="solute1" />
					<aspect name="maxConcentration" type="PRIMARY" class="Double" value="2.0" />
					<aspect name="outputWriter" type="PRIMARY" class="String" value="SvgExport" />
				</process>
				<process name="svgWriter" class="GraphicalOutput"  priority="-4" firstStep="0.0"  timerStepSize="0.2">
					<aspect name="solute" type="PRIMARY" class="String" value="solute1" />
					<aspect name="maxConcentration" type="PRIMARY" class="Double" value="1.0" />
					<aspect name="outputWriter" type="PRIMARY" class="String" value="SvgExport" />
					<aspect name="arrayType" type="PRIMARY" class="String" value="PRODUCTIONRATE" />
				</process>
				<process name="agentRelax" class="AgentRelaxation" priority="-2" firstStep="0.0"  timerStepSize="1.0">
					<aspect name="maxMovement" type="PRIMARY" class="Double" value="0.05" />
					<aspect name="relaxationMethod" type="PRIMARY" class="String" value="EULER" />
				</process>
				<process name="plasmidDynamics" class="PlasmidDynamics"  priority="-3" firstStep="0.0" timerStepSize="0.1">
					<aspect name="plasmids" type="PRIMARY" class="LinkedList">
						<item type="PRIMARY" class="String" value="betaLacRes" />
					</aspect>
				</process>
				<process name="solveDiffusionTransient" class="SolveDiffusionTransient" priority="-1" firstStep="0.0" timerStepSize="0.1" />
			</processManagers>
		</compartment>
	</simulation>
</document>