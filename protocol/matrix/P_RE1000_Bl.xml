<?xml version="1.0" encoding="UTF-8"?>
<document>
	<simulation name="P_RE1000_B" outputfolder="../results" log="DEBUG" 
						comment="this is a file for testing purposes only.">
		
		<timer stepSize="0.000001" endOfSimulation="1.0" />
		
		<speciesLib>
			<!-- species/species library The species library contains all agent species 
				in the simulations. This may include microbial species, plasmid and vector 
				species, particle species etc. Species states are states that are in principal 
				identical for all agents of that species. However note that iDynoMiCS 2.0 
				does allow you to overwrite states on agent level. Thus if a state is defined 
				both on species level and on agent level, the agent state will be used in 
				the simulation. small test -->
			<species name="bact">
				<aspect name="pigment" type="PRIMARY" class="String" value="GREEN" />
				<speciesModule name="coccoid" />
			</species>
			<species name="coccoid">
				<aspect name="surfaces"		type="CALCULATED"	class="AgentSurfaces" />
				<aspect name="morphology" 	type="PRIMARY" 		class="String" 		value="coccoid" />
				<!-- NOTE for testing purposes as stateExpression -->
				<aspect name="volume" 		type="CALCULATED" 	class="StateExpression" 	input="mass/density" />
				<aspect name="radius"		type="CALCULATED"	class="CoccoidRadius" 	/>
				<aspect name="growth" 		type="EVENT" 		class="SimpleGrowth"  		 />
				<aspect name="divide" 		type="EVENT" 		class="CoccoidDivision"			 />
				<aspect name="#isLocated" 	type="PRIMARY" 		class="Boolean" value="true" />
				<aspect name="updateBody" 	type="EVENT" 		class="UpdateBody" 		 />
				
				<aspect name="resolveForce" 	type="EVENT" 		class="aspect.event.ResolveInteractionForce"  		 />
					<aspect name="searchDist"		type="PRIMARY" class="Double"		value="0.05"  />
				<aspect name="pullDistance"		type="PRIMARY" class="Double"		value="0.05"  />
				<aspect name="evaluatePull"		type="EVENT"			class="ResolveInteractionDistance" 	 />
			</species>
		</speciesLib>

		<compartment name="second" tree="SPLITTREE">
			<shape class="Rectangle">
				<dimension name="X" isCyclic="true" targetResolution="1.0" max="30.0">
				</dimension> 
				<dimension name="Y" isCyclic="true" targetResolution="1.0" max="30.0">
				</dimension> 
			</shape>
			<solutes>
			</solutes>
			<reactions>
			</reactions>
			<agents>
				<agent>
					<aspect name="mapTest" type="PRIMARY" class="HashMap">
						<item type="PRIMARY" class="Double" value="2.0" key="1.0" keyType="Double" />
					</aspect>				
					<aspect name="species" type="PRIMARY" class="String" value="bact" />
					<aspect name="mass" type="PRIMARY" class="Double" value="0.2" />
					<aspect name="density" type="PRIMARY" class="Double" value="1" />
					<aspect name="pigment" type="PRIMARY" class="String" value="RED" />
					
					<aspect name="gammaLW" 	type="PRIMARY" 		class="Double" value="40" />
					<aspect name="gammaAcid" 	type="PRIMARY" 		class="Double" value="0.5" />
					<aspect name="gammaBase" 	type="PRIMARY" 		class="Double" value="20" />
					<aspect name="psi" 	type="PRIMARY" 		class="Double" value="-0.023" />
					
					<spawn number="100" domain="30.0,30.0" />
				</agent>
				<agent>
					<aspect name="species" type="PRIMARY" class="String" value="bact" />
					<aspect name="mass" type="PRIMARY" class="Double" value="0.2" />
					<aspect name="density" type="PRIMARY" class="Double" value="1" />
					<aspect name="pigment" type="PRIMARY" class="String" value="BLUE" />
					
					<aspect name="gammaLW" 	type="PRIMARY" 		class="Double" value="30" />
					<aspect name="gammaAcid" 	type="PRIMARY" 		class="Double" value="1.0" />
					<aspect name="gammaBase" 	type="PRIMARY" 		class="Double" value="21.0" />
					<aspect name="psi" 	type="PRIMARY" 		class="Double" value="-0.023" />
					
					<spawn number="100" domain="30.0,30.0" />
				</agent>
			</agents>
			<processManagers>
				<process name="svgWriter" class="GraphicalOutput"  priority="-3"
					firstStep="0.0"  timerStepSize="0.00000001">
					<aspect name="outputWriter" type="PRIMARY" class="String" value="SvgExport" />
				</process>
				<process name="agentRelax" class="processManager.library.LBDEM"  priority="-2"
					firstStep="0.0"  timerStepSize="0.00000001">
					<!-- speed of sound in water in Âµm / min -->
					<aspect name="speedOfSound" type="PRIMARY" class="Double"  value="89040000000.0"/>
					<!-- value="89040000000.0" value ="2.04174e10" /> -->
					<aspect name="relaxationMethod" type="PRIMARY" class="String" value="EULER" />
					<aspect name="lengthScale" type="PRIMARY" class="Double" value="100" />
					<aspect name="kinematicViscosity" type="PRIMARY" class="Double" value="60000000" />
					<aspect name="flowProfile" type="PRIMARY" class="String" value="randomProfile" />
					<aspect name="reynolds" type="PRIMARY" class="Double" value="1000" />
					
				</process>
			</processManagers>
		</compartment>
	</simulation>
</document>