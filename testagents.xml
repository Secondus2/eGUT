<?xml version="1.0" encoding="UTF-8"?>
<document>
<simulation name="mySim" comments="this is a file for Agent testing">
		<speciesLib>
			<!-- species/species library The species library contains all agent species 
				in the simulations. This may include microbial species, plasmid and vector 
				species, particle species etc. Species states are states that are in principal 
				identical for all agents of that species. However note that iDynoMiCS 2.0 
				does allow you to overwrite states on agent level. Thus if a state is defined 
				both on species level and on agent level, the agent state will be used in 
				the simulation. small test -->
			<species name="AOB">
				<aspect name="pigment" type="String" value="GREEN" />
				<!-- this has not been made yet, use a static growthrate for now -->
				<aspect name="growthRate" type="double" value="0.08" />
				<speciesModule name="coccoid" />
				<speciesModule name="stochasticMover" />
			</species>
			<species name="coccoid">
				<aspect name="coccoidCenter" type="secondary" class="CoccoidCenter"
					input="body" />
				<aspect name="morphology" type="String" value="coccoid" />
				<aspect name="joints" type="secondary" class="JointsState"
					input="body" />
				<aspect name="volume" type="secondary" class="SimpleVolumeState"
					input="mass,density" />
				<aspect name="radius" type="secondary" class="CoccoidRadius"
					input="volume" />
				<aspect name="#boundingLower" type="secondary" class="LowerBoundingBox"
					input="body,radius" />
				<aspect name="#boundingSides" type="secondary" class="DimensionsBoundingBox"
					input="body,radius" />
				<aspect name="growth" type="event" package="agent.event.library."
					class="SimpleGrowth" input="mass,growthRate" />
				<aspect name="divide" type="event" package="agent.event.library."
					class="CoccoidDivision" input="mass,radius,body" />
				<aspect name="surface" type="secondary" class="SimpleSurface"
					input="body" />
				<aspect name="updateBody" type="event" package="agent.event.library."
					class="UpdateBody" input="body,radius,length" />
			</species>
			<species name="stochasticMover">
				<aspect name="stochasticStep" type="double" value="0.1" />
				<aspect name="stochasticMove" type="event" class="StochasticMove"
					package="agent.event.library." input="body,stochasticStep" />
			</species>
		</speciesLib>
	
